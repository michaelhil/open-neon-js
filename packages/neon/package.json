{
  "name": "open-neon-js-api",
  "version": "0.1.0-beta.4",
  "type": "module",
  "description": "Auto-detecting Pupil Labs Neon eye tracker API for Node.js and browsers",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && tsc --emitDeclarationOnly",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "open-neon-js-api-core": "0.1.0-beta.4",
    "open-neon-js-api-node": "0.1.0-beta.4",
    "open-neon-js-api-browser": "0.1.0-beta.4"
  },
  "devDependencies": {
    "vite": "^7.1.3",
    "vitest": "^1.2.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
